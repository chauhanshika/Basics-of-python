print("BMI Calculator")

weight = float(input("Enter your weight (kg): "))
height = float(input("Enter your height (m): "))

bmi = weight / (height ** 2)
print(f"Your BMI is: {round(bmi, 2)}")

if bmi < 18.5:
    print("âš ï¸ Underweight")
elif 18.5 <= bmi < 24.9:
    print("âœ… Normal weight")
elif 25 <= bmi < 29.9:
    print("âš ï¸ Overweight")
else:
    print("ðŸš¨ Obese")
