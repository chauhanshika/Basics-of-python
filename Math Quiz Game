import random

print("ğŸ§  Welcome to the Math Quiz!")
score = 0

for i in range(5):
    num1 = random.randint(1, 10)
    num2 = random.randint(1, 10)
    answer = num1 + num2
    print(f"\nQuestion {i+1}: {num1} + {num2} = ?")
    user = int(input("Your answer: "))

    if user == answer:
        print("âœ… Correct!")
        score += 1
    else:
        print(f"âŒ Wrong! The correct answer is {answer}.")

print(f"\nğŸ Quiz Over! Your Score: {score}/5")
