#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int T;
    scanf("%d", &T);
    
    while (T--) {
        int n, k;
        scanf("%d %d", &n, &k);
        
        long long arr[n];
        for (int i = 0; i < n; i++) {
            scanf("%lld", &arr[i]);
        }
        
        long long count[101] = {0}; 
        count[0] = 1; 
        long long prefixSum = 0, ans = 0;
        
        for (int i = 0; i < n; i++) {
            prefixSum += arr[i];
            int rem = prefixSum % k;
            if (rem < 0) rem += k;
            
            ans += count[rem];   
            count[rem]++;        
        }
        
        printf("%lld\n", ans);
    }
    
    return 0;
}
