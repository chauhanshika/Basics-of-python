lst = []
size = int(input("Enter the number of elements: "))

for i in range(size):
    ele = int(input('Enter the element: '))
    lst.append(ele)

print('The list entered by you is:', lst)

dictt = {}
for item in lst:
    if item in dictt:
        dictt[item] += 1
    else:
        dictt[item] = 1

print("New dictionary is:", dictt)
