import random

symbols = ["ğŸ’", "ğŸ‹", "ğŸ‡", "ğŸ‰", "â­", "ğŸ’"]

print("ğŸ° Welcome to the Mini Slot Machine!")
input("Press Enter to spin... ")

slot = [random.choice(symbols) for _ in range(3)]
print(" | ".join(slot))

if len(set(slot)) == 1:
    print("ğŸ† JACKPOT! All symbols match!")
elif len(set(slot)) == 2:
    print("ğŸ˜ Nice! Two symbols match!")
else:
    print("ğŸ’¨ Better luck next time!")
